{
  "name": "banksystemweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist && mkdir dist",
    "lint": "esw  ./test ./routes ./models/ app.js bin/www ",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch",
    "test": "cross-env NODE_ENV=test mocha --exit",
    "test:report": "nyc mocha",
    "compile": "babel ./ --out-dir dist/ --ignore ./node_modules,./test --copy-files ",
    "start:prod": "cross-env NODE_ENV=dev node dist/bin/www",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec babel-node bin/www",
    "prebuild:dev": "npm-run-all test clean",
    "build:dev": "npm run compile",
    "postbuild:dev": "npm run start:prod",
    "build:release": "npm-run-all  test clean compile ",
    "postbuild:release": "rimraf dist/.git dist/test dist/nodemon.json"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "http-errors": "^1.7.3",
    "lodash": "^4.17.21",
    "mongo": "^0.1.0",
    "mongodb": "^3.3.3",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-watch": "^6.0.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  }
}
